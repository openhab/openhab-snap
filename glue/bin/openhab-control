#!/bin/bash

# set_config ${key} ${ENV} ${VALUE} ${DEFAULT} ${DESCRIPTION}
set_config() {
    local VAL=${3}
    [ -z "${VAL}" ] && VAL=${4}
    echo "${2}=${VAL}"
    eval export ${2}=\"${VAL}\"
}

source ${SNAP}/bin/snap-config
read_snap_config

# clear tmp file
rm -rf ${SNAP_DATA}/tmp/*
DIRNAME=$SNAP

exec "${DIRNAME}/runtime/bin/${@}"

