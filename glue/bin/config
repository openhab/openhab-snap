#!/bin/sh

# list of supported keys
keys="OPENHAB_HTTP_PORT OPENHAB_HTTPS_PORT"

# default values
DESCRIPTION_OPENHAB_HTTP_PORT="http port openhab is exposed on, after change restart service"
DEFAULT_OPENHAB_HTTP_PORT="8080"
KEY_OPENHAB_HTTP_PORT="http-port"

DESCRIPTION_OPENHAB_HTTPS_PORT="https port openhab is exposed on, after change restart service"
DEFAULT_OPENHAB_HTTPS_PORT="8443"
KEY_OPENHAB_HTTPS_PORT="https-port"
